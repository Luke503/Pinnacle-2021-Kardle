{
    "name": "Kardle Extension",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {
        
    },
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["jquery.js", "foreground.js"],
            "css":["styles.css"]
        }
    ],
    "background": {
        "script":["background.js"]
        
    },
    "content_security_policy": "script-src 'self' 127.0.0.1; object-src 'self'",
    

    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "storage",
        "<all_urls>"
    ]
}